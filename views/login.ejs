<% layout("/layouts/boilerplate") %>
<body>
   <br>
    <h1>Welcome to Wander Lust</h1> <br>
    <form method="POST" class="needs-validation" action="/login" novalidate>
      <div class="mb">
        <label for="username" class="form-label">Your userName</label>
        <input type="text" id="username" class="form-control" class="form-control" name="username" required placeholder="Enter username" > <br>
      </div>

      <div class="mb">
        <label for="password" class="form-label">Your password</label>
        <input type="password" id="password" class="form-control" class="form-control" name="password" required placeholder="Enter password" > <br>
      </div>

      <button class="btn btn-dark"> Login </button> 

    </form>
</body>